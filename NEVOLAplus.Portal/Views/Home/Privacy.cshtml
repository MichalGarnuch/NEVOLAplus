@inject NEVOLAplus.Portal.Services.ITextSnippetService SnippetService

@{
    ViewData["Title"] = await SnippetService.GetContentByKeyAsync("Portal_Privacy")
                       ?? "Privacy Policy";
}

<h1>@ViewData["Title"]</h1>
<p>
    @(await SnippetService.GetContentByKeyAsync("Portal_PrivacyDescription")
        ?? "Use this page to detail your site's privacy policy.")
</p>
