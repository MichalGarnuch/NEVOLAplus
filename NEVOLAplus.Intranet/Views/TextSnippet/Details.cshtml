@model NEVOLAplus.Data.Models.CMS.TextSnippet
@{
    ViewData["Title"] = "Details Snippet";
}
<h2>@ViewData["Title"]</h2>
<div>
    <dl class="row">
        <dt class="col-sm-2">Key</dt>
        <dd class="col-sm-10">@Model.Key</dd>
        <dt class="col-sm-2">Content</dt>
        <dd class="col-sm-10">@Model.Content</dd>
    </dl>
</div>
<p>
    <a asp-action="Edit" asp-route-id="@Model.TextSnippetId" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</p>
